{
  "version": 2,
  "env": {
    "GITHUB_TOKEN": ""
  },
  "functions": {
    "api/*.py": {
      "maxDuration": 30,
      "memory": 128
    }
  },
  "headers": [
    {
      "source": "/api/(.*)",
      "headers": [
        { "key": "Cache-Control", "value": "max-age=3600" },
        { "key": "Content-Type", "value": "image/svg+xml; charset=utf-8" }
      ]
    }
  ]
}
